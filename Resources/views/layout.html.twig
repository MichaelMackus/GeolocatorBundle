<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <title>{% block title %}Welcome!{% endblock %}</title>
    {% block stylesheets %}
        <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.0/css/bootstrap.min.css">
    {% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
</head>
<body>
    <div class="container">
        {% block flashes %}
            {% for name, flashes in app.session.flashbag.all if name in ['success', 'error'] %}
                {% for flash in flashes %}
                    <div class="alert alert-{{ name == 'error' ? 'danger' : name }}">
                        <button type="button" class="close" aria-hidden="true">&times;</button>
                        {{ flash|trans }}
                    </div>
                {% endfor %}
            {% endfor %}
        {% endblock %}
        {% block teneleven_geolocator_body %}{% endblock %}
    </div>
    {% block javascripts %}
        <script src="http://code.jquery.com/jquery-1.10.1.min.js"></script>
        <script src="http://demos.flesler.com/jquery/scrollTo/js/jquery.scrollTo-min.js"></script>
    {% endblock %}
</body>
</html>
